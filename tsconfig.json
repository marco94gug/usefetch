{
  "compilerOptions": {
    "target": "ES2016",
    "module": "ESNext",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true
  },
  "include": ["./"],
  "exclude": ["node_modules"]
}
